<?xml version="1.0"?>
<package format="3">
  <name>dynamic_logistics_warehouse</name>
  <version>1.0.0</version>
  <description>LARGE INDUSTRIAL WAREHOUSE</description>
  <maintainer email="belalonium@outlook.com">belalonium</maintainer>
  <license>Apache-2.0</license>

  <buildtool_depend>ament_cmake</buildtool_depend>
  
  <depend>gazebo_ros</depend>
  <depend>gazebo_plugins</depend>
  <depend>launch_ros</depend>
  <depend>launch</depend>
  
  <test_depend>ament_lint_auto</test_depend>
  <test_depend>ament_lint_common</test_depend>

  <export>
    <build_type>ament_cmake</build_type>
    <gazebo_ros plugin_path="${prefix}/lib" gazebo_media_path="${prefix}" gazebo_model_path="${prefix}/models"/>
  </export>
</package>
