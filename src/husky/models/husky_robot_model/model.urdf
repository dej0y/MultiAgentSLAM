<gazebo>
<plugin name="wheel_drive_controller" filename="libgazebo_ros_diff_drive.so">
      <num_wheel_pairs>2</num_wheel_pairs>
      <update_rate>30.0</update_rate>
      <left_joint>husky_robot_model__front_left_wheel_joint</left_joint>  
      <right_joint>husky_robot_model__front_right_wheel_joint</right_joint>
      <left_joint>husky_robot_model__rear_left_wheel_joint</left_joint>
      <right_joint>husky_robot_model__rear_right_wheel_joint</right_joint>
      <wheel_separation>0.57</wheel_separation>
      <wheel_diameter>0.34</wheel_diameter>
      <max_wheel_torque>200.0</max_wheel_torque>
      <max_wheel_acceleration>10.0</max_wheel_acceleration>      
      <odometry_frame>odom</odometry_frame>
      <robot_base_frame>husky_robot_model__base_link</robot_base_frame>

      <ros>
          <namespace>/</namespace>
          <argument>/cmd_vel:=cmd_vel</argument>
          <argument>/odom:=odom</argument>
      </ros>

      <publish_odom>true</publish_odom>
      <publish_odom_tf>true</publish_odom_tf>
    
   </plugin>
</gazebo>

